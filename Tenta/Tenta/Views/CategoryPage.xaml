<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Tenta.Views.CategoryPage">
    <ContentPage.Content>
        <StackLayout>
            <ListView x:Name="categories" ItemSelected="ButtonNavigate_Clicked">
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <TextCell Text= "{Binding }" />
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>